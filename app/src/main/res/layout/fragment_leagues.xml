<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="fragment"
            type="com.rifafauzi.footballmatch.ui.leagues.LeaguesFragment" />

        <variable
            name="vm"
            type="com.rifafauzi.footballmatch.ui.leagues.LeaguesViewModel" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.leagues.LeaguesFragment">


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvLeagues"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:listitem="@layout/list_leagues" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>